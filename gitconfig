[user]
  name = [GIT_USER_NAME]
  email = [GIT_USER_EMAIL]
[color]
  ui = auto
[core]
  attributesfile = ~/.gitattributes
  autocrlf = input
  editor = vim
  excludesfile = ~/.gitignore
  precomposeunicode = false
  trustctime = false
[fetch]
  prune = true
[help]
  autocorrect = 1
[push]
  default = current
[rebase]
  autosquash = true
